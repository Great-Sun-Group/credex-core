#!/bin/bash

/workspaces/credex-core/src/tests/update_combined_code.sh

# Add the updated combined code file
git add src/tests/combined_code.txt

# Commit the changes if there are any
if git diff --cached --exit-code; then
    echo "No changes to combined_code.txt"
else
    git commit -m "Update combined_code.txt for branch $(git rev-parse --abbrev-ref HEAD)"
fi